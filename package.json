{
  "name": "saas-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 yarn build",
    "localUmi": "node_modules/.bin/umi",
    "start": "umi dev",
    "dev": "node removeUmiLocaleMoment.js && yarn autoLoadRouteConfig && cross-env APP_NODE_ENV=development yarn localUmi dev",
    "dev:production": "yarn autoLoadRouteConfig && cross-env APP_NODE_ENV=production yarn localUmi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "eslint --fix \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "deploy": "yarn autoLoadRouteConfig && cross-env APP_NODE_ENV=production node deploy",
    "deploy:development": "yarn autoLoadRouteConfig && cross-env APP_NODE_ENV=development node deploy",
    "deploy:preProduction": "cross-env APP_NODE_ENV=preProduction node deploy",
    "build": "cross-env APP_NODE_ENV=production yarn localUmi build",
    "build:development": "cross-env APP_NODE_ENV=development yarn localUmi build",
    "build:preProduction": "cross-env APP_NODE_ENV=preProduction yarn localUmi umi build",
    "removeDist": "rm -rf ./dist",
    "coding:deploy": "cross-env APP_NODE_ENV=production node deploy/trigger-coding-job.mjs",
    "coding:deploy:dev": "cross-env APP_NODE_ENV=development node deploy/trigger-coding-job.mjs",
    "build:startDev": "yarn removeDist && yarn build:development && yarn node localServer.js",
    "autoLoadRouteConfig": "node config/route.config/auto-load.js",
    "build:startDev:notCompress": "yarn removeDist && cross-env COMPRESS=none yarn build:development && node localServer.js",
    "build:zhipinku:docker": "node ci-docker",
    "prepare": "husky install",
    "reBuildCkeditor": "cd ckeditor5 && yarn && yarn build && cd .. && yarn add file:./ckeditor5"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.6.3",
    "@ant-design/pro-layout": "^6.24.2",
    "@antv/g2plot": "^2.3.32",
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/mouse-wheel": "^2.4.2",
    "@better-scroll/observe-dom": "^2.4.2",
    "@better-scroll/scroll-bar": "^2.4.2",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@formily/antd": "^1.3.17",
    "@formily/antd-components": "^1.3.17",
    "@sentry/react": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "@spark-build/web-utils": "^0.0.7",
    "@videojs/themes": "^1.0.1",
    "ahooks": "^2.10.9",
    "antd": "4.16.13",
    "async-validator": "^4.0.2",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "ckeditor5-custom-build": "file:./ckeditor5",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "dva-model-extend": "^0.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "file-saver": "^2.0.5",
    "hash.js": "^1.1.7",
    "history": "^5.0.1",
    "immer": "^9.0.5",
    "immutability-helper": "^3.1.1",
    "js-base64": "^3.6.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.25",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "qiniu-js": "^3.3.3",
    "qrcode.react": "^1.0.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.2",
    "react-id-swiper": "^4.0.0",
    "react-image-gallery": "^1.2.7",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "redux": "^4.1.1",
    "reselect": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.0",
    "swiper": "^6.7.5",
    "umi": "3.4.25",
    "umi-request": "^1.3.9",
    "unstated-next": "^1.1.0",
    "use-immer": "^0.6.0",
    "video.js": "^7.14.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@spark-build/lint": "^0.0.17",
    "@types/amap-js-api": "^1.4.10",
    "@types/amap-js-api-district-search": "^1.4.1",
    "@types/amap-js-api-geocoder": "^1.4.1",
    "@types/amap-js-api-place-search": "^1.4.1",
    "@types/classnames": "^2.3.1",
    "@types/file-saver": "^2.0.3",
    "@types/fs-extra": "^9.0.12",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.19",
    "@types/react-color": "^3.0.5",
    "@types/react-document-title": "^2.0.5",
    "@types/react-dom": "^17.0.9",
    "@types/react-image-gallery": "^1.0.4",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-window": "^1.8.5",
    "@types/video.js": "^7.3.26",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/test": "^3.5.17",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "less-plugin-aliases": "^1.0.3",
    "lint-staged": "^11.1.2",
    "lodash-webpack-plugin": "^0.11.6",
    "mime": "^2.5.2",
    "qiniu": "^7.4.0",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^17.0.2",
    "style-resources-loader": "^1.4.1",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack-chain": "^6.5.1"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less --fix",
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  }
}
